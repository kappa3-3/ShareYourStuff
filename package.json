{
  "name": "share-your-stuff",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "easy-peasy": "^3.2.3",
    "eslint-config-airbnb": "^18.2.0",
    "formik": "^2.1.7",
    "mongodb": "^3.6.2",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.0",
    "node.normalize.scss": "^3.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropdown-select": "^3.6.8",
    "react-easy-state": "^6.1.3",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-scroll": "^1.7.14",
    "react-scroll-to": "^3.0.0-beta.3",
    "react-select": "^3.0.8",
    "redux": "^4.0.4",
    "redux-promise": "^0.6.0",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/components/**/*.js* src/pages/**/*.js*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "labelComponents": [
            "CustomInputLabel"
          ],
          "labelAttributes": [
            "label"
          ],
          "controlComponents": [
            "CustomInput"
          ],
          "depth": 3
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "__REDUX_DEVTOOLS_EXTENSION__",
            "_id"
          ]
        }
      ],
      "react/jsx-props-no-spreading": 0,
      "arrow-parens": [
        "error",
        "as-needed"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
